<h1><%= movie.title %></h1>

<div class="spacer"></div>

<div class="text-right">
  <%= link_to movies_path(movie), :class =>"seen" do %>
    <i class="glyphicon glyphicon-trash"></i> Watched it
  <% end %>
</div>
<div class="text-center videoWrapper clearfix">
  <%= movie.embed.html_safe %>
</div>
<div class="text-center">
  <%= link_to "Find someone to see it with!", movies_path(movie), :class=>"btn btn-info" %>
</div>



<div class="spacer"></div>